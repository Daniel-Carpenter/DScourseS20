output$scatter <- renderCachedPlot(
{
ggplot(mtcars,
aes(.data[[input$x]],
.data[[input$y]])) +
geom_point() +
theme_minimal()
},
cacheKeyExpr = list(input$x, input$y))
## Render Exact Dastaset
output$table <- renderTable(
{
print(df)
})
}
